{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "share-purse-frontend",
  "main": ".open-next/worker.js",
  "compatibility_date": "2024-09-26",
  "compatibility_flags": ["nodejs_compat"],
  "minify": true,
  "assets": {
    "binding": "ASSETS",
    "directory": ".open-next/assets"
  },
  "observability": {
    "enabled": true
  },
  "vars": {
    "NEXT_PUBLIC_BACKEND_URL": "https://api.share-purse.workers.dev"
  },
  "env": {
    "staging": {
      "name": "share-purse-frontend-staging",
      "vars": {
        "NEXT_PUBLIC_BACKEND_URL": "https://api-staging.share-purse.workers.dev"
      }
    }
  }
}
